{
  "name": "arini",
  "version": "1.0.0",
  "description": "The way web-apps are supposed to be made.",
  "main": "index.js",
  "scripts": {
    "generate": "jison src/grammar/arini.jison src/grammar/arini.jisonlex -o src/grammar/parser.js",
    "babel": "babel src --out-dir lib --source-maps inline",
    "buildServe": "browserify -d packages/serve/serve-client.js -o packages/serve/client.js",
    "buildMan": "node ./man/createMan.js",
    "build": "npm-run-all --sequential generate babel buildServe buildMan",
    "start": "babel-node index.js",
    "sc": "node index.js",
    "test": "mocha",
    "debug": "node inspect ./bin/cli.js"
  },
  "bin": {
    "arini": "./lib/bin/runFile.js"
  },
  "man": [
    "./man/arini.1"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-p-n/arini.git"
  },
  "keywords": [
    "programming",
    "language"
  ],
  "author": "Spencer A. Lockhart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s-p-n/arini/issues"
  },
  "homepage": "https://github.com/s-p-n/arini#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "browserify-fs": "^1.0.0",
    "chalk": "^1.1.3",
    "combine-source-map": "^0.8.0",
    "convert-source-map": "^1.5.1",
    "express": "^4.16.3",
    "hyperscript": "^2.0.2",
    "js-beautify": "^1.7.5",
    "meow": "^5.0.0",
    "mongojs": "^2.6.0",
    "shelljs": "^0.8.2",
    "socket.io": "^2.1.1",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.6",
    "spdy": "^3.4.7",
    "xregexp": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.2",
    "babel-preset-minify": "0.5.0",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "dateformat": "^3.0.3",
    "jison": "^0.4.18",
    "man-api": "^0.3.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "uglify-js": "^3.4.2"
  }
}
