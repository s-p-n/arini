let foo, bar, baz;

// array insertion
foo = [];
foo[] = 'a';
foo[] = 'b';
foo[] = 'c';

// array inline creation and deletion.
bar = ['a', 'b', 'c', 'd', 'e', 'f', 'g'];
bar.shift(); // remove first index- 'a'
bar.pop(); // remove last index- 'g'
bar.splice(0,2); // remove first 2 indices: 'b' and 'c'

// join 2 arrays
baz = foo.concat(bar);
return [foo, bar, baz];